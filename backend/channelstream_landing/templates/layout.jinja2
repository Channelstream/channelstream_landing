<!DOCTYPE html>
<html lang="{{ request.locale_name }}">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="pyramid web application">
    <meta name="author" content="Pylons Project">
    <link rel="shortcut icon"
          href="{{ request.static_url('channelstream_landing:static/pyramid-16x16.png') }}">

    <title>Channelstream</title>


    <!-- Custom styles for this scaffold -->
    <link href="{{ request.static_url('channelstream_landing:static/theme.css') }}"
          rel="stylesheet">
    <link href="{{ request.static_url('channelstream_landing:static_bundled/css/main.css') }}"
          rel="stylesheet">
    <script src="{{ request.static_url('channelstream_landing:static_bundled/node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js') }}"></script>
</head>

<body>

<div>
    {% block content %}
        <p>No content</p>
    {% endblock content %}
</div>

<div class="links">
    <ul>
        <li><i class="glyphicon glyphicon-cog icon-muted"></i><a
                href="https://github.com/Channelstream/channelstream">Github Project</a></li>
        <li><i class="glyphicon glyphicon-home icon-muted"></i><a
                href="https://github.com/Channelstream/channelstream/issues">Issue Tracker</a></li>
    </ul>
</div>


<div class="row">
    <div class="copyright">
        Copyright &copy; Channelstream Project
    </div>
</div>


</body>
</html>
